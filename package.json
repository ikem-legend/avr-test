{
  "name": "avenir",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullcalendar/bootstrap": "^4.3.0",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/list": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@iconscout/react-unicons": "0.0.1",
    "apexcharts": "^3.8.6",
    "availity-reactstrap-validation": "^2.6.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.9.0",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "jwt-decode": "^2.2.0",
    "metismenujs": "^1.1.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-albus": "^2.0.0",
    "react-apexcharts": "^1.3.3",
    "react-bootstrap-table-next": "^3.2.0",
    "react-bootstrap-table2-paginator": "^2.1.0",
    "react-bootstrap-table2-toolkit": "^2.1.0",
    "react-cookie": "^4.0.1",
    "react-device-detect": "^1.9.9",
    "react-dom": "^16.9.0",
    "react-draft-wysiwyg": "^1.13.2",
    "react-dropzone": "^10.1.9",
    "react-feather": "^2.0.3",
    "react-flatpickr": "^3.9.1",
    "react-loadable": "^5.5.0",
    "react-perfect-scrollbar": "^1.5.3",
    "react-plaid-link": "^1.5.0",
    "react-redux": "^7.1.1",
    "react-router-auth": "^1.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "react-select": "^3.0.5",
    "react-sortablejs": "^1.5.1",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^5.5.0",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "sortablejs": "^1.10.0"
  },
  "scripts": {
    "start": "set PORT=3002 && react-scripts start",
    "build": "react-scripts build",
    "test": "kcd-scripts test",
    "lint": "kcd-scripts lint",
    "validate": "kcd-scripts validate && npm run test:e2e:run",
    "setup": "npm install --silent && npm run validate --silent",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "babel": {
    "presets": [
      "kcd-scripts/babel"
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "import/prefer-default-export": "off",
      "jsx-a11y/label-has-for": "off",
      "react/prop-types": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "jsx-a11y/accessible-emoji": "off",
      "consistent-return": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "proseWrap": "always"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@jackfranklin/test-data-bot": "github:jackfranklin/test-data-bot",
    "@testing-library/cypress": "^5.3.0",
    "cypress": "^4.0.2",
    "eslint-plugin-cypress": "^2.10.1",
    "is-ci-cli": "^2.0.0",
    "jest-axe": "^3.2.0",
    "kcd-scripts": "^1.14.0",
    "start-server-and-test": "^1.10.8"
  }
}
